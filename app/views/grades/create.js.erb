<%unless @grade_exists%>
    <% inlineform_action = params[:add_more] ? "inlineform:save_and_add_more" : "inlineform:save" %>
    <%= "{\"action\": \"#{inlineform_action}\",\"exists\": \"#{@grade_exists}\", \"record\": \"<option value='#{@grade.id}' selected type='#{params[:association] == "account" ? "account_level" : "company_level"}'>#{@grade.title}</option>\"}".html_safe %>
<%else%>
    <%= "{\"exists\": \"#{@grade_exists}\"}".html_safe %>
<%end%>
